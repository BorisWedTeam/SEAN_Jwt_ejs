<!-- Right Panel -->
<div id="right-panel" class="right-panel">

        <%- include include/topnav.ejs %>
        <div class="breadcrumbs">
            <div class="col-sm-4">
                <div class="page-header float-left">
                    <div class="page-title">
                        <h1>Dashboard</h1>
                    </div>
                </div>
            </div>
            <div class="col-sm-8">
                <div class="page-header float-right">
                    <div class="page-title">
                        <ol class="breadcrumb text-right">
                            <li class="active">Dashboard</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>

        <div class="content mt-3">
            <h2 class="text-center" style="margin-bottom:20px;">Please select a customer</h2>
            <div class="col-md-4"></div>
            <div class="col-md-4 text-center">
                <% user.customers.forEach(function(customer){ %>
                    <a href="#" id="btnCustomer<%=customer.id %>" class="btn btn-primary btnCustomer" onclick="selectCustomer(<%=customer.id %>)"><%=customer.name%></a>
                <% }); %>
            </div>
        </div> <!-- .content -->
    </div><!-- /#right-panel -->